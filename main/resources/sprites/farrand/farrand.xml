<animations        width="32"        height="32"	 >    <animation            name="walk_front"            loop="true"            frames="4"            row="0"            default_speed="150ms"/>    <animation        name="walk_back"        loop="true"        frames="4"        row="1"        default_speed="150ms"/>    <animation            name="walk_left"            loop="true"            frames="4"            row="2"            default_speed="150ms"/>    <animation            name="walk_right"            loop="true"            frames="4"            row="3"            default_speed="150ms" />	<animation			name="swipe_down"			loop="true"			frames="5"			row="4"			default_speed="0ms" />	<animation			name="dodge_down"			loop="false"			frames="4"			row="5"			default_speed="0ms" />	<animation			name="swipe_left"			loop="false"			frames="5"			row="6"			default_speed="0ms" />    <animation            name="swipe_right"            loop="false"            frames="5"            row="7"            default_speed="0ms" /></animations>		