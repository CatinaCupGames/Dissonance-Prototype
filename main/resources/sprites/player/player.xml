<animations        width="32"        height="32"        max_frames="2"	 >    <animation            name="walk_front"            loop="true"            frames="2"            row="0"            default_speed="10ms" />    <animation            name="walk_back"            loop="true"            frames="1"            row="1"            default_speed="10ms" />    <animation            name="walk_left"            loop="true"            frames="1"            row="2"            default_speed="10ms" />    <animation            name="walk_right"            loop="true"            frames="1"            row="3"            default_speed="10ms" /></animations>		