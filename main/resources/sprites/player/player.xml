<animations        width="32"        height="32"	 >    <animation            name="walk_front"            loop="true"            frames="4"            row="0"            default_speed="150ms" />    <animation            name="walk_back"            loop="true"            frames="1"            row="1"            default_speed="10ms" />    <animation            name="walk_left"            loop="true"            frames="1"            row="2"            default_speed="10ms" />    <animation            name="walk_right"            loop="true"            frames="1"            row="3"            default_speed="10ms" />	<animation			name="sword_stab"			loop="false"			frames="4"			row="4"			default_speed="500ms" />	<animation			name="sword_swipe"			loop="false"			frames="4"			row="5"			default_speed="500ms" />				<animation			name="dodge"			loop="false"			frames="4"			row="6"			default_speed="375ms" /></animations>		